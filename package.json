{
  "name": "@coriolis/dev-tools",
  "version": "1.0.0-alpha2",
  "description": "Dev tools for coriolis",
  "license": "GPL-3.0",
  "author": "Nicolas Deveaud",
  "repository": "github:coriolisjs/dev-tools",
  "bugs": "https://github.com/coriolisjs/dev-tools/issues",
  "keywords": [
    "coriolis",
    "event",
    "eventsourcing",
    "effect",
    "aggregate",
    "projection",
    "reducer",
    "predictable",
    "functional",
    "immutable",
    "hot",
    "live",
    "replay",
    "store",
    "dev-tools",
    "devtools",
    "dev",
    "tools",
    "debug"
  ],
  "main": "dist/cjs/index.js",
  "sideEffects": false,
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "clear:all": "rm -rf node_modules dist",
    "clear": "rm -rf dist",
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "dev": "run-p start:dev autobuild",
    "start": "sirv public --single",
    "start:dev": "sirv public --single --dev",
    "lint": "eslint .",
    "pretty": "prettier './**/{*.{js?(on),md,?(s)css,html},.*.{js?(on),md,?(s)css,html}}' --write && eslint . --fix",
    "upgrade": "npm outdated --parseable --depth=0 | cut -d: -f4 | xargs npm install",
    "dep-link": "npm link @coriolis/coriolis @coriolis/coriolis-svelte @coriolis/parametered-projection",
    "prepublishOnly": "npm run lint && npm outdated && npm run clear && npm run build && git tag ${VERSION} && git push origin ${VERSION}",
    "publish:local": "VERSION=${npm_package_version}-local npm publish --registry http://localhost:4873",
    "publish:npm": "VERSION=${npm_package_version} npm publish --registry https://registry.npmjs.org --access public"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.2.1",
    "rollup": "^2.56.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.38.2",
    "sirv-cli": "^1.0.14",
    "svelte": "^3.42.4",
    "svelte-preprocess": "^4.8.0"
  },
  "dependencies": {
    "@coriolis/coriolis": "file:../coriolis",
    "@coriolis/coriolis-svelte": "file:../coriolis-svelte",
    "@coriolis/parametered-projection": "file:../parametered-projection",
    "@sveltejs/svelte-virtual-list": "^3.0.1",
    "@webcomponents/custom-elements": "^1.5.0",
    "immer": "^9.0.5",
    "json-format-highlight": "^1.0.4",
    "rxjs": "^7.3.0"
  }
}
